    if (self.x+objCamera.scrollX+xSpeed&gt;objCamera.x+256) //Shorten the laser as it gets too far off the screen
    {
        length-=3;
    }
    
    if length&lt;=0 //As the laser ends entirely off the screen, self-destruct to clear memory
    {
