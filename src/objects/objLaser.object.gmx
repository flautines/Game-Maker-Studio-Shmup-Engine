    }
    with (parentGun)
        {

            switch(myID)
            { //Find out which turret this laser came from so we can set x/y position directly
            case 0:
                other.y = y-1 + vsp+objCamera.scrollY;
                break;
            case 1:            
                other.y = myPlayer.prevY[9]-1;
                break;
            case 2:            
